<h1>Add New Player</h1>
<%= form_for @player, :url=>party_players_path do |f| %>
  <fieldset>
    <legend>Player Details</legend>
    <ul>
      <li>
        <%= f.label :name %>:
        <%= f.text_field :name %>
      </li>
      <li>
        <%= f.label :tripcode %>
        <%= f.text_field :tripcode %>
      </li>
    </ul>
  </fieldset>
  <%= f.submit  %>
<% end %>
